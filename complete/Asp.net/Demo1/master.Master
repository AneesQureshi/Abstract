<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="master.master.cs" Inherits="Demo1.master" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title>

        <asp:ContentPlaceHolder ID="contenttitle" runat="server">
